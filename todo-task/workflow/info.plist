				<string># Create the data dir and TODO file if it doesn't exist already
if [ ! -d ~/Library/Application\ Support/Alfred\ 3/Workflow\ Data/nikivi.create.task ]; then
	mkdir ~/Library/Application\ Support/Alfred\ 3/Workflow\ Data/nikivi.create.task    
	touch ~/Library/Application\ Support/Alfred\ 3/Workflow\ Data/nikivi.create.task/todo
	touch ~/Library/Application\ Support/Alfred\ 3/Workflow\ Data/nikivi.create.task/temp
fi

# cd to to the Alfred dir
cd ~/Library/Application\ Support/Alfred\ 3/Workflow\ Data/nikivi.create.task

# Remove the file
rm -rf todo 

# Write the TODO task to the file
echo -e "$1" &gt; todo</string>
